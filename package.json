{
  "name": "sms-hub",
  "version": "0.0.1",
  "description": "SMS Number and Message Management",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "format": "prittier --write \"**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "start": "node ./dist/server.js",
    "test": "jest",
    "watch": "nodemon"
  },
  "author": "Simon Skinner <ss@beyondcomparison.com>",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "2.205.0",
    "body-parser": "1.18.2",
    "dotenv": "4.0.0",
    "express": "4.17.3",
    "pg-promise": "5.9.7",
    "request": "2.83.0",
    "typescript": "2.7.2"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.8",
    "@types/dotenv": "4.0.2",
    "@types/express": "4.11.1",
    "@types/node": "9.4.6",
    "jest": "23.1.0",
    "nodemon": "1.17.5",
    "prettier": "1.13.5",
    "ts-jest": "22.4.6",
    "ts-node": "7.0.0",
    "tslint": "5.10.0"
  },
  "private": true,
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../docs",
    "coverageReporters": [
      "html"
    ]
  }
}
